    "import pandas as pd\n",
    "data = pd.read_csv('playerdata.csv')\n",
    "p=data['profit']\n",
    "def get_stats(group):\n",
    "    return {'count':group.count(),'mean':group.mean()}\n",
    "factor=pd.cut(p,6)\n",
    "grouped=p.groupby(factor)\n",
    "grouped.apply(get_stats).unstack()"
   ]
